{
  "name": "AI Professional",
  "version": "3.0.0",
  "description": "Professional theme for AI Investment Platform with modern gradients and optimal contrast",
  "colors": {
    "primary": "#2563eb",
    "secondary": "#1e40af",
    "accent": "#3b82f6",
    "success": "#10b981",
    "warning": "#f59e0b",
    "error": "#ef4444",
    "info": "#06b6d4",
    "background": "#0f172a",
    "surface": "#1e293b",
    "card": "#334155",
    "text": {
      "primary": "#f1f5f9",
      "secondary": "#cbd5e1",
      "muted": "#94a3b8",
      "disabled": "#64748b"
    },
    "border": {
      "primary": "#475569",
      "secondary": "#374151",
      "accent": "#3b82f6"
    },
    "gradient": {
      "primary": "linear-gradient(135deg, #2563eb 0%, #1e40af 100%)",
      "secondary": "linear-gradient(135deg, #1e293b 0%, #334155 100%)",
      "accent": "linear-gradient(135deg, #3b82f6 0%, #2563eb 100%)",
      "card": "linear-gradient(135deg, #1e293b 0%, #334155 50%, #475569 100%)",
      "success": "linear-gradient(135deg, #10b981 0%, #059669 100%)",
      "warning": "linear-gradient(135deg, #f59e0b 0%, #d97706 100%)",
      "error": "linear-gradient(135deg, #ef4444 0%, #dc2626 100%)"
    }
  },
  "typography": {
    "fontFamily": {
      "primary": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
      "secondary": "'Fira Code', 'Monaco', 'Cascadia Code', monospace",
      "display": "'Inter', sans-serif"
    },
    "fontSize": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem"
    },
    "fontWeight": {
      "normal": "400",
      "medium": "500",
      "semibold": "600",
      "bold": "700"
    },
    "lineHeight": {
      "tight": "1.25",
      "normal": "1.5",
      "relaxed": "1.75"
    }
  },
  "spacing": {
    "xs": "0.25rem",
    "sm": "0.5rem",
    "md": "1rem",
    "lg": "1.5rem",
    "xl": "2rem",
    "2xl": "3rem",
    "3xl": "4rem"
  },
  "borderRadius": {
    "none": "0",
    "sm": "0.25rem",
    "md": "0.375rem",
    "lg": "0.5rem",
    "xl": "0.75rem",
    "full": "9999px"
  },
  "shadows": {
    "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
    "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
    "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
    "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
    "glow": "0 0 20px rgba(59, 130, 246, 0.3)"
  },
  "components": {
    "button": {
      "primary": {
        "background": "linear-gradient(135deg, #2563eb 0%, #1e40af 100%)",
        "color": "#ffffff",
        "border": "1px solid #3b82f6",
        "borderRadius": "0.5rem",
        "padding": "0.75rem 1.5rem",
        "fontSize": "0.875rem",
        "fontWeight": "600",
        "boxShadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)",
        "transition": "all 0.2s ease-in-out",
        "hover": {
          "background": "linear-gradient(135deg, #1e40af 0%, #1e3a8a 100%)",
          "boxShadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1)",
          "transform": "translateY(-1px)"
        }
      },
      "secondary": {
        "background": "linear-gradient(135deg, #374151 0%, #4b5563 100%)",
        "color": "#f1f5f9",
        "border": "1px solid #6b7280",
        "borderRadius": "0.5rem",
        "padding": "0.75rem 1.5rem",
        "fontSize": "0.875rem",
        "fontWeight": "500"
      }
    },
    "card": {
      "background": "linear-gradient(135deg, #1e293b 0%, #334155 50%, #475569 100%)",
      "border": "1px solid #475569",
      "borderRadius": "0.75rem",
      "padding": "1.5rem",
      "boxShadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1)",
      "backdropFilter": "blur(10px)"
    },
    "input": {
      "background": "#334155",
      "border": "1px solid #475569",
      "borderRadius": "0.5rem",
      "padding": "0.75rem 1rem",
      "color": "#f1f5f9",
      "fontSize": "0.875rem",
      "focus": {
        "border": "1px solid #3b82f6",
        "boxShadow": "0 0 0 3px rgba(59, 130, 246, 0.1)"
      }
    },
    "modal": {
      "background": "linear-gradient(135deg, #1e293b 0%, #334155 100%)",
      "border": "1px solid #475569",
      "borderRadius": "1rem",
      "boxShadow": "0 25px 50px -12px rgba(0, 0, 0, 0.25)",
      "backdropFilter": "blur(16px)"
    },
    "table": {
      "headerBackground": "#374151",
      "rowBackground": "#1e293b",
      "rowHover": "#334155",
      "border": "#475569"
    }
  },
  "animations": {
    "duration": {
      "fast": "150ms",
      "normal": "200ms",
      "slow": "300ms"
    },
    "easing": {
      "default": "cubic-bezier(0.4, 0, 0.2, 1)",
      "in": "cubic-bezier(0.4, 0, 1, 1)",
      "out": "cubic-bezier(0, 0, 0.2, 1)",
      "bounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)"
    }
  },
  "breakpoints": {
    "sm": "640px",
    "md": "768px",
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1536px"
  }
}