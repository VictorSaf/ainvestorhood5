{
  "name": "AIInvestorHood Default Theme",
  "version": "1.0.0",
  "colors": {
    "primary": {
      "50": "#eff6ff",
      "100": "#dbeafe", 
      "200": "#bfdbfe",
      "300": "#93c5fd",
      "400": "#60a5fa",
      "500": "#3b82f6",
      "600": "#2563eb",
      "700": "#1d4ed8",
      "800": "#1e40af",
      "900": "#1e3a8a"
    },
    "secondary": {
      "50": "#f8fafc",
      "100": "#f1f5f9",
      "200": "#e2e8f0", 
      "300": "#cbd5e1",
      "400": "#94a3b8",
      "500": "#64748b",
      "600": "#475569",
      "700": "#334155",
      "800": "#1e293b",
      "900": "#0f172a"
    },
    "success": {
      "50": "#f0fdf4",
      "100": "#dcfce7",
      "200": "#bbf7d0",
      "300": "#86efac", 
      "400": "#4ade80",
      "500": "#22c55e",
      "600": "#16a34a",
      "700": "#15803d",
      "800": "#166534",
      "900": "#14532d"
    },
    "danger": {
      "50": "#fef2f2",
      "100": "#fee2e2",
      "200": "#fecaca",
      "300": "#fca5a5",
      "400": "#f87171", 
      "500": "#ef4444",
      "600": "#dc2626",
      "700": "#b91c1c",
      "800": "#991b1b",
      "900": "#7f1d1d"
    },
    "warning": {
      "50": "#fffbeb",
      "100": "#fef3c7",
      "200": "#fde68a",
      "300": "#fcd34d",
      "400": "#fbbf24",
      "500": "#f59e0b",
      "600": "#d97706",
      "700": "#b45309", 
      "800": "#92400e",
      "900": "#78350f"
    },
    "accent": {
      "50": "#faf5ff",
      "100": "#f3e8ff",
      "200": "#e9d5ff",
      "300": "#d8b4fe",
      "400": "#c084fc",
      "500": "#a855f7",
      "600": "#9333ea",
      "700": "#7c3aed",
      "800": "#6b21a8",
      "900": "#581c87"
    }
  },
  "typography": {
    "fontFamily": {
      "sans": ["Inter", "system-ui", "-apple-system", "BlinkMacSystemFont", "Segoe UI", "Roboto", "sans-serif"],
      "mono": ["SF Mono", "Monaco", "Inconsolata", "Roboto Mono", "monospace"]
    },
    "fontSize": {
      "xs": "0.75rem",
      "sm": "0.875rem", 
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem"
    },
    "fontWeight": {
      "normal": "400",
      "medium": "500",
      "semibold": "600",
      "bold": "700",
      "extrabold": "800"
    },
    "lineHeight": {
      "tight": "1.25",
      "normal": "1.5", 
      "relaxed": "1.75"
    }
  },
  "spacing": {
    "xs": "0.25rem",
    "sm": "0.5rem",
    "md": "1rem",
    "lg": "1.5rem",
    "xl": "2rem",
    "2xl": "3rem",
    "3xl": "4rem"
  },
  "borderRadius": {
    "none": "0",
    "sm": "0.125rem",
    "md": "0.375rem",
    "lg": "0.5rem",
    "xl": "0.75rem",
    "2xl": "1rem",
    "full": "9999px"
  },
  "shadows": {
    "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
    "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
    "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
    "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
    "2xl": "0 25px 50px -12px rgba(0, 0, 0, 0.25)"
  },
  "components": {
    "button": {
      "primary": {
        "background": "linear-gradient(135deg, #3b82f6 0%, #8b5cf6 100%)",
        "color": "#ffffff",
        "border": "none",
        "borderRadius": "0.5rem",
        "padding": "0.5rem 1rem",
        "fontSize": "0.875rem",
        "fontWeight": "600",
        "boxShadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)",
        "hover": {
          "background": "linear-gradient(135deg, #2563eb 0%, #7c3aed 100%)",
          "boxShadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1)"
        }
      },
      "secondary": {
        "background": "#f1f5f9",
        "color": "#334155",
        "border": "1px solid #e2e8f0",
        "borderRadius": "0.5rem",
        "padding": "0.5rem 1rem",
        "fontSize": "0.875rem",
        "fontWeight": "500",
        "hover": {
          "background": "#e2e8f0",
          "borderColor": "#cbd5e1"
        }
      },
      "success": {
        "background": "#22c55e",
        "color": "#ffffff",
        "border": "none",
        "borderRadius": "0.5rem",
        "padding": "0.5rem 1rem",
        "fontSize": "0.875rem",
        "fontWeight": "600",
        "hover": {
          "background": "#16a34a"
        }
      },
      "danger": {
        "background": "#ef4444",
        "color": "#ffffff", 
        "border": "none",
        "borderRadius": "0.5rem",
        "padding": "0.5rem 1rem",
        "fontSize": "0.875rem",
        "fontWeight": "600",
        "hover": {
          "background": "#dc2626"
        }
      }
    },
    "card": {
      "default": {
        "background": "#ffffff",
        "border": "1px solid #e5e7eb",
        "borderRadius": "0.5rem",
        "boxShadow": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
        "padding": "1.5rem"
      },
      "hover": {
        "boxShadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
        "transform": "translateY(-1px)"
      }
    },
    "input": {
      "default": {
        "background": "#ffffff",
        "border": "1px solid #d1d5db",
        "borderRadius": "0.5rem",
        "padding": "0.75rem 1rem",
        "fontSize": "0.875rem",
        "color": "#374151",
        "focus": {
          "borderColor": "#3b82f6",
          "boxShadow": "0 0 0 3px rgba(59, 130, 246, 0.1)"
        }
      },
      "error": {
        "borderColor": "#ef4444",
        "background": "#fef2f2",
        "focus": {
          "borderColor": "#ef4444",
          "boxShadow": "0 0 0 3px rgba(239, 68, 68, 0.1)"
        }
      }
    },
    "modal": {
      "overlay": {
        "background": "rgba(0, 0, 0, 0.5)",
        "backdropFilter": "blur(4px)"
      },
      "content": {
        "background": "#ffffff",
        "borderRadius": "0.75rem",
        "boxShadow": "0 25px 50px -12px rgba(0, 0, 0, 0.25)",
        "maxWidth": "32rem",
        "padding": "1.5rem"
      }
    },
    "badge": {
      "primary": {
        "background": "#dbeafe",
        "color": "#1e40af",
        "padding": "0.25rem 0.75rem",
        "borderRadius": "9999px",
        "fontSize": "0.75rem",
        "fontWeight": "600"
      },
      "success": {
        "background": "#dcfce7",
        "color": "#166534",
        "padding": "0.25rem 0.75rem",
        "borderRadius": "9999px",
        "fontSize": "0.75rem",
        "fontWeight": "600"
      },
      "danger": {
        "background": "#fee2e2",
        "color": "#991b1b",
        "padding": "0.25rem 0.75rem",
        "borderRadius": "9999px",
        "fontSize": "0.75rem",
        "fontWeight": "600"
      },
      "warning": {
        "background": "#fef3c7",
        "color": "#92400e",
        "padding": "0.25rem 0.75rem",
        "borderRadius": "9999px",
        "fontSize": "0.75rem",
        "fontWeight": "600"
      }
    },
    "newsCard": {
      "container": {
        "background": "#ffffff",
        "border": "1px solid #e5e7eb",
        "borderRadius": "0.75rem",
        "padding": "1.5rem",
        "marginBottom": "1px",
        "hover": {
          "boxShadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1)"
        }
      },
      "title": {
        "fontSize": "1.125rem",
        "fontWeight": "600",
        "color": "#111827",
        "lineHeight": "1.5",
        "marginBottom": "0.75rem"
      },
      "summary": {
        "fontSize": "0.875rem",
        "color": "#6b7280",
        "lineHeight": "1.5",
        "marginBottom": "1rem"
      },
      "meta": {
        "fontSize": "0.75rem",
        "color": "#9ca3af",
        "marginBottom": "1rem"
      }
    },
    "header": {
      "container": {
        "background": "rgba(255, 255, 255, 0.95)",
        "backdropFilter": "blur(12px)",
        "border": "1px solid rgba(229, 231, 235, 0.5)",
        "padding": "1rem"
      },
      "title": {
        "fontSize": "1.25rem",
        "fontWeight": "700",
        "background": "linear-gradient(135deg, #2563eb 0%, #7c3aed 100%)",
        "backgroundClip": "text",
        "color": "transparent"
      },
      "statusIndicator": {
        "online": {
          "background": "#dcfce7",
          "color": "#166534",
          "borderRadius": "9999px",
          "padding": "0.25rem 0.75rem",
          "fontSize": "0.75rem",
          "fontWeight": "600"
        },
        "offline": {
          "background": "#fee2e2",
          "color": "#991b1b",
          "borderRadius": "9999px",
          "padding": "0.25rem 0.75rem",
          "fontSize": "0.75rem",
          "fontWeight": "600"
        }
      }
    },
    "tabs": {
      "container": {
        "background": "#ffffff",
        "border": "1px solid #e5e7eb",
        "borderRadius": "0.5rem"
      },
      "tab": {
        "active": {
          "color": "#2563eb",
          "borderBottom": "2px solid #2563eb",
          "fontWeight": "600"
        },
        "inactive": {
          "color": "#6b7280",
          "borderBottom": "2px solid transparent",
          "fontWeight": "500",
          "hover": {
            "color": "#374151",
            "borderBottom": "2px solid #d1d5db"
          }
        }
      }
    },
    "progressBar": {
      "container": {
        "background": "#f3f4f6",
        "borderRadius": "9999px",
        "height": "0.5rem"
      },
      "fill": {
        "background": "linear-gradient(135deg, #3b82f6 0%, #8b5cf6 100%)",
        "borderRadius": "9999px",
        "height": "100%"
      }
    }
  },
  "animations": {
    "fadeIn": {
      "duration": "0.5s",
      "easing": "ease-in-out",
      "keyframes": {
        "0%": { "opacity": "0", "transform": "translateY(10px)" },
        "100%": { "opacity": "1", "transform": "translateY(0)" }
      }
    },
    "slideUp": {
      "duration": "0.3s", 
      "easing": "ease-out",
      "keyframes": {
        "0%": { "transform": "translateY(100%)" },
        "100%": { "transform": "translateY(0)" }
      }
    },
    "pulse": {
      "duration": "2s",
      "easing": "cubic-bezier(0.4, 0, 0.6, 1)",
      "iterationCount": "infinite",
      "keyframes": {
        "0%, 100%": { "opacity": "1" },
        "50%": { "opacity": "0.5" }
      }
    },
    "spin": {
      "duration": "1s",
      "easing": "linear",
      "iterationCount": "infinite",
      "keyframes": {
        "0%": { "transform": "rotate(0deg)" },
        "100%": { "transform": "rotate(360deg)" }
      }
    }
  },
  "gradients": {
    "primary": "linear-gradient(135deg, #3b82f6 0%, #8b5cf6 100%)",
    "secondary": "linear-gradient(135deg, #f8fafc 0%, #f1f5f9 100%)",
    "success": "linear-gradient(135deg, #22c55e 0%, #16a34a 100%)",
    "danger": "linear-gradient(135deg, #ef4444 0%, #dc2626 100%)",
    "warning": "linear-gradient(135deg, #f59e0b 0%, #d97706 100%)",
    "accent": "linear-gradient(135deg, #a855f7 0%, #7c3aed 100%)",
    "background": "linear-gradient(135deg, #f8fafc 0%, #f1f5f9 100%)"
  },
  "breakpoints": {
    "sm": "640px",
    "md": "768px", 
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1536px"
  }
}